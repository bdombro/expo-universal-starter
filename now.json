{
  "name": "EUSY",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "web-build" }
    }
  ],
  "routes": [
    {
      "src": "/static/media/(.*)",
      "headers": { "cache-control": "s-maxage=604800" },
      "dest": "/static/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "version": 2
}
